<?xml version="1.0"?>
<!DOCTYPE Plugin PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Plugin name="systemhealthplugin" displayName="System Health" version="0.0.5" disabled="false" certificationLevel="None" rightRequired="SystemHealthPluginRight" minSystemVersion="8.1"
	maxSystemVersion="8.4">
	<Attributes>
		<Map>
			<entry key="snippets">
				<value>
					<List>
						<Snippet regexPattern=".*" rightRequired="SystemHealthPluginIconRight">
							<Scripts>
								<String>ui/js/headerInject.js</String>
							</Scripts>
							<StyleSheets>
								<String>ui/css/systemHealth.css</String>
							</StyleSheets>
						</Snippet>
					</List>
				</value>
			</entry>
			<entry key="minUpgradableVersion" value="0.0.1.0" />
			<entry key="restResources">
				<value>
					<List>
						<String>sailpoint.plugin.systemhealthplugin.rest.HealthResource</String>
					</List>
				</value>
			</entry>
		</Map>
	</Attributes>
</Plugin>
