<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<sailpoint>
    <ImportAction name="merge">
        <Configuration name="PluginFrameworkConfig">
            <Attributes>
                <Map>
                    <entry key="Plugins">
                        <value>
                            <Map>
                                <entry key="SystemHealthPlugin">
                                    <value>
                                        <Plugin uniqueName="systemhealthplugin" displayName="System Health" enabled="true"
                                                version="0.0.0.21" allowDisable="true" visible="true" allowUninstall="true"
                                                minFrameworkVersion="0.4.5" maxFrameworkVersion="1.0.0"
                                                minUpgradableVersion="0.0.0.1" certificationLevel="BASIC"
                                                pluginAccessRight="SystemAdministrator">

                                            <PluginConfiguration>
                                                <Snippets>
                                                    <Snippet pagesMatchRegExPattern=".*" isMobile="true" isDesktop="true" rightRequired="SystemAdministrator" >
                                                        <ScriptIncludes>
                                                            <String>{plugin_root}/ui/js/headerInject.js</String>
                                                        </ScriptIncludes>
                                                        <StylesheetIncludes>
                                                            <String>{plugin_root}/ui/css/systemHealth.css</String>
                                                        </StylesheetIncludes>
                                                    </Snippet>
                                                </Snippets>
                                            </PluginConfiguration>
                                        </Plugin>
                                    </value>
                                </entry>
                            </Map>
                        </value>
                    </entry>
                </Map>
            </Attributes>
        </Configuration>
    </ImportAction>
</sailpoint>
